{
    "presets": [
        "es2015",
        "react",
        "stage-0",
        "flow"
    ],
    "plugins": [
        ["import", { "libraryName": "antd", "style": true }],
        "add-module-exports",
        "transform-decorators-legacy",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
    ]
}
