#confirm
  position fixed
  left 0
  top 0
  background-color rgba(0,0,0,.5)
  width 100%
  height 100%
  z-index 9
  .confirmBox
    width 288px
    height 180px
    margin 10% auto
    background #fff
    overflow hidden
    .confirmContent
      text-align center
      margin 50px auto 30px
      font-size 14px
    .confirmBtn
      width 248px
      height 42px
      margin 0 auto
      div
        width 114px
        height 42px
        float left
        text-align center
        color #fff
        line-height 42px
        cursor pointer
      .sumbit
        background #009aec
      .cancel
        background #b4b4b4
      .midBtn
        margin 0 65px
